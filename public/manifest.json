{
  "name": "AliMonitor",
  "description": "Generate universally unique identifiers fast.",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/zanozbot/uuid-generator",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "AliMonitor",
    "default_icon": {
      "48": "logo48.png",
      "96": "logo96.png"
    }
  },
  "icons": {
    "48": "logo48.png",
    "96": "logo96.png"
  },
  "permissions": [
    "http://alimonitor.cern.ch/*"
  ]
}
