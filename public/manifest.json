{
  "name": "AliMonitor",
  "description": "AliMonitor Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "homepage_url": "https://gitlab.cern.ch/kwijethu/alimonitor-browser-extension",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "AliMonitor",
    "default_icon": {
      "48": "./images/favicon.jpg",
      "96": "./images/favicon.jpg"
    }
  },
  "icons": {
    "48": "./images/favicon.jpg",
    "96": "./images/favicon.jpg"
  },
  "permissions": [
    "http://alimonitor.cern.ch/*"
  ]
}
